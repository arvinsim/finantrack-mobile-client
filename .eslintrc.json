{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
      "standard",
      "standard-react"
    ],
    "plugins": [
      "react",
      "react-native"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true
      }
    }
}